{
  "name": "12beesinatrenchcoat.github.io",
  "version": "7.0.0",
  "description": "a personal website",
  "private": "true",
  "main": "index.js",
  "scripts": {
    "sass": "npx sass ./style/index.sass ./_site/style/index.css",
    "watch:sass": "npx sass ./style/index.sass ./_site/style/index.css --watch",
    "watch:eleventy": "npx @11ty/eleventy --serve",
    "build": "pnpm run sass & npx @11ty/eleventy",
    "dev": "pnpm run \"/^watch:.*/\""
  },
  "author": "Andy Chan / 12beesinatrenchcoat <andy@12beesinatrenchco.at> (12beesinatrenchco.at)",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^2.0.0",
    "eleventy-plugin-toc": "^1.1.5",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.6",
    "markdown-it-attrs": "^4.1.6",
    "sass": "^1.58.1"
  },
  "devDependencies": {
    "@types/markdown-it": "^12.2.3"
  }
}
